{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.sln" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": ""
        }
      ],
      "dest": "api",
      "properties": { "TargetFramework": "netstandard2.0" }
    }
  ],
 "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
	"globalMetadata": {
	  "_appTitle": "iU880B-USB",	
      "_enableSearch": true,
      "_gitContribute": {
        "orgName": "fosterbuster",
        "repoName": "iU880B-USB",
        "repo": "https://github.com/fosterbuster/iU880B-USB.git",
        "branch": "master"
      },
	  "_gitUrlPattern": "github"
    },
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}